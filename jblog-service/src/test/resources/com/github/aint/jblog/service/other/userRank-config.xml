<!DOCTYPE configuration PUBLIC "//UNKNOWN/" "userRank-config.dtd">
<configuration>
  <rating-range min="-1000" max="-100">
    <rank name="mega troll" />
  </rating-range>
  <rating-range min="-99" max="-1">
    <rank name="troll" />
  </rating-range>
  <rating-range min="0" max="25">
    <rank name="junior" />
  </rating-range>
  <rating-range min="26" max="99">
    <rank name="middle" />
  </rating-range>
  <rating-range min="100" max="1000">
    <rank name="senior" />
  </rating-range>
</configuration>
